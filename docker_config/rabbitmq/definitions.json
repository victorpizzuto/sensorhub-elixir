{
    "rabbit_version": "3.12.0",
    "users": [
        {
            "name": "username",
            "password": "password",
            "tags": "administrator"
        }
    ],
    "vhosts": [
        {
            "name": "/"
        }
    ],
    "permissions": [
        {
            "user": "username",
            "vhost": "/",
            "configure": ".*",
            "write": ".*",
            "read": ".*"
        }
    ],
    "queues": [
        {
            "name": "sensor_data",
            "vhost": "/",
            "durable": true
        }
    ],
    "bindings": [
        {
            "source": "amq.topic",
            "vhost": "/",
            "destination": "sensor_data",
            "destination_type": "queue",
            "routing_key": "dev.*.read"
        }
    ]
}